{
  "name": "oscar-portfolio",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.6.0",
    "npm": ">=3.10.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "nodemon --config nodemon.json --exec babel-node server.js",
    "dev": "NODE_ENV=development npm start",
    "prod": "NODE_ENV=production npm run vendors && NODE_ENV=production npm run build",
    "vendors": "NODE_ENV=development webpack --config=webpack/vendors.babel.js",
    "build": "webpack --config=webpack/config.babel.js -p",
    "dump_db": "mongodump --host localhost:27017 -o dump",
    "restore_db": "mongorestore --db oscar-granse dump/oscar-granse"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "dotenv": "^1.1.0",
    "gsap": "^1.18.0",
    "jade": "^1.11.0",
    "keystone": "git://github.com/keystonejs/keystone.git#master",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-helmet": "^3.2.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^1.0.2",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.x",
    "eslint-plugin-import": "^1.4.x",
    "eslint-plugin-jsx-a11y": "^0.6.x",
    "eslint-plugin-react": "^4.3.x",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gridle": "^2.0.48",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "import-glob-loader": "^1.1.0",
    "morgan": "^1.7.0",
    "multer": "^0.1.8",
    "node-sass": "^3.9.3",
    "nodemon": "^1.11.0",
    "normalize.css": "^4.2.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^0.13.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "rimraf": "^2.5.4",
    "sass-easing": "^1.0.3",
    "sass-font-face": "^1.0.1",
    "sass-loader": "^4.0.2",
    "serve": "^1.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.2",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-validator": "^2.2.7"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "transform-regenerator"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "airbnb",
    "globals": {
      "TweenMax": false,
      "Cubic": false,
      "Expo": false,
      "Linear": false,
      "Power2": false,
      "Power3": false,
      "Power4": false,
      "Quad": false
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "no-console": 0
    }
  },
  "config": {
    "verbose": false,
    "port": 3000,
    "browsers": [
      "last 2 version",
      "safari 5",
      "ie 7",
      "ie 8",
      "ie 9",
      "opera 12.1",
      "ios 6",
      "android 4"
    ],
    "prodURL": "http://us.spencediamonds.com",
    "shareImageURL": "http://domain.tld/static/share.png",
    "twitterHandle": "@shopspence",
    "analyticsUA": "UA-1440822-1",
    "developerURL": ""
  }
}
